{
  "name": "hyperf",
  "version": "1.5.0",
  "description": "Hypertext fragments with reactivity",
  "main": "src/index.js",
  "module": "src/index.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "hyper fragments",
    "hyperscript",
    "jsx",
    "dom-jsx",
    "fragment",
    "htl",
    "htm",
    "hyps",
    "reactivity",
    "reactive",
    "template",
    "build-dom",
    "observable",
    "rxjs"
  ],
  "scripts": {
    "test": "node --expose-gc -r ./test/register.cjs test/index.js",
    "start": "serve",
    "build": "rollup src/index.js -p @rollup/plugin-node-resolve --file hyperf.js --format esm",
    "minify": "terser hyperf.js -o hyperf.min.js --module -c passes=3 -m"
  },
  "files": [
    "src",
    "hyperf.js",
    "hyperf.min.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spectjs/hyperf.git"
  },
  "author": "Dmitry Iv",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spectjs/hyperf/issues"
  },
  "homepage": "https://github.com/spectjs/hyperf#readme",
  "dependencies": {
    "element-props": "^2.0.4",
    "htm": "^3.1.0",
    "sube": "^2.2.1",
    "swapdom": "^1.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "es-module-shims": "^1.4.1",
    "jsdom": "^19.0.0",
    "rollup": "^2.64.0",
    "terser": "^5.10.0",
    "tst": "^7.1.0",
    "value-ref": "^2.1.0",
    "wait-please": "^3.1.0"
  }
}
